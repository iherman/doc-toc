{
    "extends": "airbnb-base",
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1,
                "CallExpression": {
                    "arguments": "first"
                }
            }
        ],
        "no-multi-spaces": [
            "error",
            {
                "exceptions": {
                    "VariableDeclarator": true
                }
            }
        ],
        "no-console": "off",
        "camelcase": "off",
        "strict": "off",
        "no-undef": "error",
        "env" {
            "browser":true
        },
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "key-spacing": [
            "error",
            {
                "align": {
                    "beforeColon": true,
                    "afterColon": true,
                    "on": "colon"
                }
            }
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "arrow-parens": [
            "error",
            "always"
        ],
        "no-plusplus": [
            "error",
            {
                "allowForLoopAfterthoughts": true
            }
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "prefer-destructuring": "off",
        "consistent-return": "off",
        "no-eval": "error",
        "no-implied-eval": "error"
    }
}
